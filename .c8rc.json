{
  "all": true,
  "src": "src",
  "include": [
    "dist/src/**/*.js"
  ],
  "exclude": [
    "dist/test/**",
    "dist/scripts/**",
    "dist/peg/**",
    "**/*.test.js",
    "**/*.d.ts"
  ],
  "reporter": [
    "text",
    "text-summary",
    "html",
    "lcov",
    "json"
  ],
  "reports-dir": "coverage",
  "temp-directory": ".nyc_output",
  "clean": true,
  "check-coverage": true,
  "lines": 75,
  "functions": 75,
  "branches": 70,
  "statements": 75,
  "per-file": false,
  "skip-full": false,
  "watermarks": {
    "lines": [70, 85],
    "functions": [70, 85],
    "branches": [65, 80],
    "statements": [70, 85]
  }
}
